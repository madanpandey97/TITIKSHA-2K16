				<script type="text/javascript">

			function reg(eve){
				var sid="";
				if(sid=="NA")
				{
					alert("Must register to participate events!.");
				}else{
					if (window.XMLHttpRequest)
					{// code for IE7+, Firefox, Chrome, Opera, Safari
						xmlhttp=new XMLHttpRequest();
					}
					else
					{// code for IE6, IE5
						xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
					}
					xmlhttp.onreadystatechange=function()
					{
						if (xmlhttp.readyState==4 && xmlhttp.status==200)
						{
							document.getElementById(eve).innerHTML = xmlhttp.responseText;
						}
					}
					xmlhttp.open("GET.html","reg5644.html?event="+eve+"&id=",true);
					xmlhttp.send();
				}
            }
</script>
<script type="text/javascript" >
function bgm_ctrl()
{
	if(document.getElementById('bgm_img').name=="un")
		{	
			document.getElementById('bgm').play();
			document.getElementById('bgm_img').name="mu";
			document.getElementById('bgm_img').src="images/unmute.png";
			document.getElementById('bgm_img').title="Mute Background Music.";
		}else
		{	document.getElementById('bgm').pause();
			document.getElementById('bgm_img').name="un";
			document.getElementById('bgm_img').src="images/mute.png";
			document.getElementById('bgm_img').title="Unmute Background Music.";
		}
}
</script>
